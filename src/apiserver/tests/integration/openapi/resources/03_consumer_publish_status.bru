meta {
  name: get_consumer_status
  type: http
  seq: 22
}

get {
  url: {{scheme}}://{{host}}/api/v1/open/gateways/{{gateway_name}}/resources/consumers/{{consumer_id}}/status/
}

headers {
  Content-Type: application/json
  X-BK-API-TOKEN: {{x_bk_api_token}}
}

assert {
  res.status: eq 200
  res.body.data.status: eq success
  res.body.data.id: matches /^bk.*/
}
