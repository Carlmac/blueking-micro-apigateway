}

headers {
  X-BK-API-TOKEN: {{x_bk_api_token}}
  Content-Type: application/json
}

body {
  {
    "name": "test_service-222",
    "config": {
      "upstream": {
        "type": "roundrobin",
        "nodes": [
          {
            "host": "httpbin.org",
            "port": 443,
            "weight": 1
          }
        ]
      }
    }
  }
}

assert {
  res.status: eq 200
}
